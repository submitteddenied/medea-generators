class <%= class_name %> < Medea::JasonObject
  <%
    result = []
    ["has", "owns"].each do |l|
      if list_properties[l]
        list_properties[l].each do |a|
           result << "#{l}_many :#{a[0]}, #{a[1]}"
        end
      end
    end
  %>
  <%= result.join("\n  ") if result.count > 0 %>
end